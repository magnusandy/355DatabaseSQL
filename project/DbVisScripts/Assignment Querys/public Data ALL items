SELECT
    i_inumkey as "Numeric Key",
    i_ialphakey as "Alphabetical Key",
    i_clientkey as  "Client Key",
    i_iname as "Item name",
    i_iorigin "Place of Origin" ,
    i_iformat "Format",
    i_isubformat "Sub-Format",
    i_ischool "School of Work",
    i_isubject "Subject of Work",
    i_iacquisitiondate "Date Aquired",
    i_icreationyear "Date Created",
    i_idescription "Item Description"
FROM
    public.v_public_data_items